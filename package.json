{
  "name": "@marko/tags-api-preview",
  "description": "A glimpse into the future of Marko.",
  "version": "0.5.0",
  "author": "Dylan Piercey <dpiercey@ebay.com>",
  "bugs": "https://github.com/marko-js/tags-api-preview/issues",
  "dependencies": {
    "magic-string": "^0.26.1"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.4",
    "@changesets/cli": "^2.22.0",
    "@marko/compiler": "^5.21.3",
    "@marko/fixture-snapshots": "^2.1.7",
    "@marko/testing-library": "^6.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/babel__code-frame": "^7.0.3",
    "@types/chai": "^4.3.1",
    "@types/chai-as-promised": "^7.1.5",
    "@types/chai-dom": "0.0.13",
    "@types/jsdom": "^16.2.14",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.38",
    "@types/sinon-chai": "^3.2.8",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "chai-dom": "^1.11.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.14.42",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "fast-glob": "^3.2.11",
    "fixpack": "^4.0.0",
    "fs-monkey": "^1.0.3",
    "husky": "^8.0.1",
    "jsdom": "^19.0.0",
    "jsdom-context-require": "^4.0.4",
    "lint-staged": "^13.0.0",
    "marko": "^5.21.2",
    "memfs": "^3.4.4",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "prettier": "^2.6.2",
    "sinon": "^14.0.0",
    "sinon-chai": "^3.7.0",
    "tsx": "^3.4.2",
    "typescript": "^4.7.2",
    "unionfs": "^4.4.0"
  },
  "files": [
    "dist",
    "marko.json",
    "!**/__tests__",
    "!**/*.tsbuildinfo"
  ],
  "homepage": "https://github.com/marko-js/tags-api-preview",
  "keywords": [
    "api",
    "marko",
    "preview",
    "tags"
  ],
  "license": "MIT",
  "main": "marko.json",
  "repository": {
    "type": "git",
    "url": "https://github.com/marko-js/tags-api-preview"
  },
  "scripts": {
    "build": "tsc -b && tsx build.mts",
    "change": "changeset add",
    "ci:test": "nyc npm run mocha -- --forbid-pending --forbid-only",
    "format": "npm run lint:eslint -- --fix && npm run lint:prettier -- --write && (fixpack || true)",
    "lint": "tsc -b && npm run lint:eslint && npm run lint:prettier -- -l && fixpack",
    "lint:eslint": "eslint -f visualstudio .",
    "lint:prettier": "prettier \"./**/*{.ts,.js,.json,.md,.yml,rc}\"",
    "mocha": "cross-env NODE_ENV=test mocha \"./src/**/__tests__/**/*.test.ts\"",
    "postpublish": "tsx publish.mts post",
    "prepare": "husky install",
    "publish": "npm run build && tsx publish.mts pre && changeset publish",
    "report": "open ./coverage/lcov-report/index.html",
    "test": "npm run mocha -- --watch",
    "test:inspect": "npm test -- --inspect",
    "test:update": "npm run mocha -- --update",
    "version": "changeset version && npm i --package-lock-only"
  }
}
